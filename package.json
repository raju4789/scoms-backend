{
  "name": "scoms-backend",
  "version": "1.0.0",
  "description": "ScreenCloud Order Management System Backend",
  "main": "dist/index.js",
  "scripts": {
    "// Development": "====================================",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:down:dev": "docker-compose -f docker-compose.dev.yml down",
    "dev": "NODE_ENV=development npx ts-node src/index.ts",
    "dev:watch": "NODE_ENV=development npx ts-node-dev --respawn --transpile-only src/index.ts",
    "// Production": "====================================",
    "build": "tsc",
    "docker:prod": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "start": "node dist/index.js",
    "// Utility": "====================================",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "consul": "^2.0.1",
    "dotenv": "^16.5.0",
    "express": "^4.17.1",
    "http-status-codes": "^2.3.0",
    "pg": "^8.16.0",
    "pino": "^9.7.0",
    "prom-client": "^15.1.0",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.24",
    "typescript": "^5.8.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@jest/types": "^29.6.3",
    "@types/consul": "^2.0.0",
    "@types/eslint": "^9.6.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.26",
    "@types/pg": "^8.15.2",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "jest": "^29.7.0",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.5.3",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node-dev": "^2.0.0"
  }
}
